{
  "name": "super-waka-land",
  "version": "1.0.0",
  "description": "Super Waka Land",
  "source": "src/index.html",
  "browserslist": "> 0.5%, last 2 versions, not dead",
  "scripts": {
    "clean": "rm -rf .parcel-cache && rm -rf dist && mkdir dist",
    "copy-assets": "cp -r src/assets/sounds/* dist",
    "prestart": "npm run clean",
    "start": "npm run copy-assets && npx parcel --hmr-port 1236",
    "prebuild": "npm run clean",
    "build": "npx parcel build --public-url '.'",
    "postbuild": "npm run copy-assets",
    "deploy": "npm run build && gh-pages -d dist"
  },
  "keywords": [
    "waka",
    "wakaland",
    "rat",
    "pixi",
    "pixijs",
    "infinite",
    "runner",
    "2d",
    "retro"
  ],
  "author": "MewK",
  "dependencies": {
    "@picocss/pico": "^1.5.2",
    "@pixi/particle-emitter": "^5.0.7",
    "@pixi/sound": "^4.2.0",
    "@pixi/tilemap": "^3.2.2",
    "eventemitter3-timer": "^1.0.1",
    "mjolnir.js": "^2.6.0",
    "pixi-filters": "^4.1.6",
    "pixi.js": "^6.3.2",
    "stats.js": "^0.17.0"
  },
  "devDependencies": {
    "@parcel/optimizer-data-url": "^2.6.0",
    "@parcel/packager-xml": "^2.6.0",
    "@parcel/transformer-inline-string": "^2.6.0",
    "@parcel/transformer-xml": "^2.6.0",
    "gh-pages": "^4.0.0",
    "parcel": "latest",
    "querystring-es3": "^0.2.1"
  }
}
